{
  "name": "rabiloo/ip",
  "description": "An Ip library for PHP",
  "keywords": [
    "rabiloo",
    "ip"
  ],
  "homepage": "https://github.com/rabiloo/php-ip",
  "license": "MIT",
  "authors": [
    {
      "name": "Oanh Nguyen",
      "email": "oanhnn.bk@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.6",
    "vimeo/psalm": "^4.11"
  },
  "autoload": {
    "psr-4": {
      "Rabiloo\\Ip\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "format": "vendor/bin/phpcbf",
    "format:check": "vendor/bin/phpcs",
    "psalm": "vendor/bin/psalm"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
